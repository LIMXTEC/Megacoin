QMenu {
    background-color: #AF272E;
    border: 0px solid black;
}

QMenu::item {
    background-color: transparent;
	color: #deeef1;
    padding: 4px 10px 4px 20px;
}

QMenu::item:selected {
    background-color: #9D0B17;
	color: #E9D9D8;
    border: none;
}

/* ================= combobox */
QComboBox {
    border: 1px solid #ededed;
    padding: 0px 18px 0px 3px;
    min-width: 20px;
	min-height: 20px;
 }

 QComboBox:editable {
     background: white;
 }

 QComboBox:!editable{
      background-color: #fafcfc;
 }

QComboBox:disabled {
	background-color: #e0e0e0;
}

 QComboBox:!editable:on {
     background-color: #fafcfc;
 }

 QComboBox:on {
     /*padding-top: 3px;
     padding-left: 4px;*/
 }

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
	background-color: #BB9396;

    border-left-width: 1px;
    border-left-color: darkgray;
    border-left-style: solid;
}
QComboBox::drop-down:hover, QComboBox::drop-down:on {
	background-color: #9F7174;
}

 QComboBox::down-arrow {
	image: url(:/res/combo_arrow.png);
 }

 QComboBox::down-arrow:on {
/*     top: 1px;
     left: 1px;*/
 }

/* =========== QLineEdit =============*/
QLineEdit {
	border: 0px solid gray;
}
QLineEdit:disabled {
	background-color: #e0e0e0;
}
/* =========== header ===============*/
QWidget[is_header=&quot;true&quot;] {
	background-color: #AB7F82;
}
/* =========== CheckBox ==============*/
QCheckBox {
    spacing: 5px;
	color: #AF272E;
}

QCheckBox::indicator {
    width: 12px;
    height: 12px;
}

QCheckBox::indicator:unchecked {
	image: url(:/res/check_unchecked.png);
}

QCheckBox::indicator:unchecked:hover {
    image: url(:/res/check_unchecked.png);
}

QCheckBox::indicator:unchecked:pressed {
    image: url(:/res/check_unchecked.png);
}

QCheckBox::indicator:checked {
